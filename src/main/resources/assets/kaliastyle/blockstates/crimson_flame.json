{
	"multipart": [
		{   "when": {"north": false, "east": false, "south": false, "west": false, "up": false},
			"apply": [
				{ "model": "kaliastyle:crimson_flame_floor0" },
				{ "model": "kaliastyle:crimson_flame_floor1" }
			]
		},
		{   "when": {"OR": [{"north": true}, {"north": false, "east": false, "south": false, "west": false, "up": false}]},
			"apply": [
				{ "model": "kaliastyle:crimson_flame_side0" },
				{ "model": "kaliastyle:crimson_flame_side1" },
				{ "model": "kaliastyle:crimson_flame_side_alt0" },
				{ "model": "kaliastyle:crimson_flame_side_alt1" }
			]
		},
		{   "when": {"OR": [{"east": true}, {"north": false, "east": false, "south": false, "west": false, "up": false}]},
			"apply": [
				{ "model": "kaliastyle:crimson_flame_side0", "y": 90 },
				{ "model": "kaliastyle:crimson_flame_side1", "y": 90 },
				{ "model": "kaliastyle:crimson_flame_side_alt0", "y": 90 },
				{ "model": "kaliastyle:crimson_flame_side_alt1", "y": 90 }
			]
		},
		{   "when": {"OR": [{"south": true}, {"north": false, "east": false, "south": false, "west": false, "up": false}]},
			"apply": [
				{ "model": "kaliastyle:crimson_flame_side0", "y": 180 },
				{ "model": "kaliastyle:crimson_flame_side1", "y": 180 },
				{ "model": "kaliastyle:crimson_flame_side_alt0", "y": 180 },
				{ "model": "kaliastyle:crimson_flame_side_alt1", "y": 180 }
			]
		},
		{   "when": {"OR": [{"west": true}, {"north": false, "east": false, "south": false, "west": false, "up": false}]},
			"apply": [
				{ "model": "kaliastyle:crimson_flame_side0", "y": 270 },
				{ "model": "kaliastyle:crimson_flame_side1", "y": 270 },
				{ "model": "kaliastyle:crimson_flame_side_alt0", "y": 270 },
				{ "model": "kaliastyle:crimson_flame_side_alt1", "y": 270 }
			]
		},
		{   "when": {"up": true},
			"apply": [
				{ "model": "kaliastyle:crimson_flame_up0" },
				{ "model": "kaliastyle:crimson_flame_up1" },
				{ "model": "kaliastyle:crimson_flame_up_alt0" },
				{ "model": "kaliastyle:crimson_flame_up_alt1" }
			]
		}
	]
}
